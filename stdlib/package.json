{
    "name": "@flyde/stdlib",
    "version": "0.1.12",
    "description": "Flyde's StdLib",
    "flyde": {
        "exposes": [
            "parts/*.flyde",
            "parts/*.flyde.js",
            "parts/**/*.flyde.js",
            "parts/**/*.flyde",
            "dist/**/*.flyde.js"
        ],
        "typings": "flyde-typings.d.ts"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "echo": "echo 'no watch'",
        "watch:ts": "tsc -p . -w --incremental",
        "watch:docs-data": "nodemon --watch scripts --watch src -e ts --exec \"ts-node scripts/docs-data.ts\"",
        "dev": "concurrently --kill-others \"yarn watch:ts\" \"yarn watch:docs-data\"",
        "build": "tsc -p . && yarn build:docs-data",
        "build:docs-data": "ts-node scripts/docs-data.ts",
        "prod": "node dist/index.js",
        "__publish": "npm version patch && npm publish"
    },
    "author": "Flyde",
    "license": "MIT",
    "dependencies": {
        "@flyde/core": "^0.1.12",
        "@types/pg": "^8.6.6",
        "axios": "^0.27.2",
        "mongodb": "^4.13.0",
        "pg": "^8.8.0",
        "pubsub-js": "^1.9.4"
    },
    "devDependencies": {
        "concurrently": "^7.3.0",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^1.0.0-pre.32",
        "typescript": "^4.9.4"
    },
    "gitHead": "5f06bd4a0f3da610e64d74c19cc84babc4557f56"
}
