  "scripts": {
    "docusaurus": "docusaurus",
    "start:docs": "concurrently 'yarn build && yarn serve' 'watch -p \"src/**/*\" -p \"../core/src/**/*\" -c \"yarn build && yarn serve\"'",
    "start": "TYPEDOC_WATCH=true concurrently 'docusaurus start --port 3003'",
    "flyde": "flyde --port=8545",
    "dev": "concurrently 'yarn start' 'yarn watch:stdlib-docs' --kill-others",
    "__publish": "npm version patch && npm publish",
    "watch:stdlib-docs": "nodemon --watch ../stdlib/dist/parts.json --exec \"yarn build:stdlib-docs\"",
    "build:stdlib-docs": "ts-node scripts/build-stdlib-docs.ts",
    "build": "docusaurus build && yarn build:stdlib-docs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "test": "mocha --require ts-node/register 'src/**/*.spec.ts'"
  },
